{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 5 sites at XM Media",
    "require": {
        "php": "8.4.*",
        "craftcms/ckeditor": "^4.4.0",
        "craftcms/cms": "5.5.6.1",
        "craftcms/contact-form": "3.1.0",
        "craftcms/contact-form-honeypot": "2.1.0",
        "ether/seo": "5.0.0",
        "hybridinteractive/craft-contact-form-extensions": "5.0.0",
        "mmikkel/cp-field-inspect": "^2.0.2",
        "nystudio107/craft-vite": "^5.0",
        "roave/security-advisories": "dev-master",
        "verbb/field-manager": "4.0.3",
        "vlucas/phpdotenv": "^5.4",
        "wrav/oembed": "3.1.3"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\imagemodule\\": "modules/imagemodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}