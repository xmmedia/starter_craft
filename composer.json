{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 4 sites at XM Media",
    "require": {
        "php": "8.1.*",
        "clubstudioltd/craft-asset-rev": "^7.0",
        "craftcms/ckeditor": "3.10.0",
        "craftcms/cms": "4.13.2",
        "craftcms/contact-form": "3.1.0",
        "craftcms/contact-form-honeypot": "2.1.0",
        "ether/seo": "4.2.4",
        "hybridinteractive/craft-contact-form-extensions": "4.2.2",
        "mmikkel/cp-field-inspect": "^1.4",
        "roave/security-advisories": "dev-master",
        "spicyweb/craft-neo": "4.2.21",
        "verbb/field-manager": "3.0.9",
        "vlucas/phpdotenv": "^5.4",
        "wrav/oembed": "3.1.1"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\imagemodule\\": "modules/imagemodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}