{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 3 sites at XM Media",
    "require": {
        "php": "7.3.*",
        "acoustep/craft-easymde": "^0.1.7",
        "clubstudioltd/craft-asset-rev": "^6.0",
        "craftcms/cms": "^3.5.10",
        "craftcms/contact-form": "2.2.7",
        "craftcms/contact-form-honeypot": "1.0.2",
        "craftcms/redactor": "^2.7",
        "hybridinteractive/craft-contact-form-extensions": "1.2.2",
        "vlucas/phpdotenv": "^3.4.0",
        "wrav/oembed": "^1.3"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3"
        }
    }
}
