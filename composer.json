{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 3 sites at XM Media",
    "require": {
        "php": "8.0.*",
        "clubstudioltd/craft-asset-rev": "^6.0",
        "craftcms/cms": "3.7.42",
        "craftcms/contact-form": "2.5.1",
        "craftcms/contact-form-honeypot": "1.0.3",
        "craftcms/redactor": "2.10.8",
        "hybridinteractive/craft-contact-form-extensions": "1.3.1",
        "roave/security-advisories": "dev-master",
        "verbb/field-manager": "2.2.4",
        "vlucas/phpdotenv": "^3.4",
        "wrav/oembed": "1.3.15"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
