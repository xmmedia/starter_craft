{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 5 sites at XM Media",
    "require": {
        "php": "8.5.*",
        "ext-intl": "*",
        "craftcms/ckeditor": "4.11.0",
        "craftcms/cms": "^5.9.10",
        "craftcms/contact-form": "3.1.0",
        "craftcms/contact-form-honeypot": "2.1.0",
        "ether/seo": "5.0.0",
        "hybridinteractive/craft-contact-form-extensions": "5.0.0",
        "nystudio107/craft-vite": "^5.0",
        "roave/security-advisories": "dev-master",
        "verbb/field-manager": "4.0.3",
        "vlucas/phpdotenv": "^5.4",
        "wrav/oembed": "3.2.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\xmmodule\\": "modules/xmmodule/src/",
            "modules\\imagemodule\\": "modules/imagemodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}
