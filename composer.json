{
    "name": "xm/starter_craft_3",
    "license": "MIT",
    "type": "project",
    "description": "Starter for creating Craft 3 sites at XM Media",
    "require": {
        "php": "8.0.*",
        "clubstudioltd/craft-asset-rev": "^7.0",
        "craftcms/cms": "^4.0",
        "craftcms/contact-form": "^3.0",
        "craftcms/contact-form-honeypot": "^2.0",
        "craftcms/redactor": "^3.0.2",
        "ether/seo": "^4.0",
        "hybridinteractive/craft-contact-form-extensions": "^4.2",
        "roave/security-advisories": "dev-master",
        "spicyweb/craft-neo": "^3.3",
        "verbb/field-manager": "^3.0",
        "vlucas/phpdotenv": "^3.4",
        "wrav/oembed": "^2.2"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\imagemodule\\": "modules/imagemodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
